                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        f.ID AS 'ftrid', 1 AS relorders, s.EMAIL, CAST(s.REMARKS AS VARCHAR(MAX)) AS 'REMARKS', ph.PLID,
                             (SELECT        SUM(WEIGHT) AS Expr1
                               FROM            TBL_PALLETHEADERS
