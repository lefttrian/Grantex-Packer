                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select m.subcode1,left(m.subcode1,9) itemtype,m.description, sum(s.primaryqty) sum from ITEMTRANSEST s inner join material m on m.id=s.ITEID 
left join fintrade f on f.id=s.ftrid
left join tbl_packerordercheck poc on poc.ftrid=f.id
left join STORETRADE AS ST ON ST.FTRID = f.id
where f.dsrid=9000  and poc.status&lt;12 and poc.status&gt;=6 and left(m.code,3) in 
(select codeid from FLDCUSTBL1 where PKRTBL_ITEMTYPESID in(select value from F_Split(@Param3, ','))) 
and  (ST.m_dispatchdate &gt;= @Param1) AND (ST.m_dispatchdate &lt;= @Param2)
group by m.subcode1,m.description
order by 3 desc</CommandText>
